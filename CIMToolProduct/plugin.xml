<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="4.28"?>
<plugin>

   <extension
         id="ide"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="CIMTool 2.2.0">
         <property
               name="windowImages"
               value="icons/cimtool-v2_16x16.png,icons/cimtool-v2_32x32.png,icons/cimtool-v2_48x48.png,icons/cimtool-v2_64x64.png,icons/cimtool-v2_128x128.png,icons/cimtool-v2_256x256.png">
         </property>
         <property
               name="aboutText"
               value="CIMTool is an open source tool that supports the Common Information Model (CIM) standards used in the electric power industry.  It is potentially useful for other semantic applications too.">
         </property>
         <property
               name="aboutImage"
               value="cimtool-about.png">
         </property>
         <property
               name="startupProgressRect"
               value="0,300,500,20">
         </property>
         <property
               name="startupMessageRect"
               value="50,260,400,20">
         </property>
         <property
               name="appName"
               value="CIMTool 2.2.0">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
      </product>
   </extension>

</plugin>
